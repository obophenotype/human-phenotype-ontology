PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?syn_type (count(?lab) as ?count) WHERE {
	?x rdf:type owl:Class .
	?x ?syn_type ?lab . 
  VALUES ?syn_type { <http://www.geneontology.org/formats/oboInOwl#hasExactSynonym> <http://www.geneontology.org/formats/oboInOwl#hasRelatedSynonym> <http://www.geneontology.org/formats/oboInOwl#hasBroadSynonym> <http://www.geneontology.org/formats/oboInOwl#hasNarrowSynonym>} 
	FILTER(isIRI(?x) && regex(str(?x), "http://purl.obolibrary.org/obo/HP_"))
	FILTER NOT EXISTS {?x owl:deprecated ?y}
}
GROUP BY ?syn_type
